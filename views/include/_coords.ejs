<% for(var i = 0; i < coords.length; i++) { %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= coords[i].locationName %></td>
    <td><%= coords[i].latitude %></td>
    <td><%= coords[i].longitude %></td>
    <td>
        <a href="/admin/coord-manager/update-coord?id=<%= coords[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= coords[i]._id%> >
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% } %>