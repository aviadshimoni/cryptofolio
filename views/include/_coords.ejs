<% for(var i = 0; i < coords.length; i++) { %>
<tr>
    <td><%= coords[i]._id %></td>
    <td><%= coords[i].locationName %></td>
    <td><%= coords[i].latitude %></td>
    <td><%= coords[i].longitude %></td>
    <td>
        <a href="/admin/coord-manager/update-coord?id=<%= coords[i]._id%>" class="btn border-shadow update btn-light" role="button" aria-pressed="true">
            <i class="bi bi-pen"></i>
        </a>
        <a href="/admin/coin-manager/update-coin?id=<%= coords[i]._id%>" class="btn border-shadow update btn-light" role="button" aria-pressed="true">
            <i class="bi bi-trash3"></i>
        </a>
    </td>
</tr>
<% } %>
